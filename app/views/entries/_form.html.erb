<%= form_for @entry, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <%= f.label :starting_lane, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :starting_lane, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :entry_count, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :entry_count, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :paid, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.check_box :paid, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :rejected_count, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :rejected_count, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :refunded, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.check_box :refunded, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :average, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :average, :class => 'text-field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  entries_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
