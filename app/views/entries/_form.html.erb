<%= simple_form_for(@entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :bracket_group %>
    <%= f.input :starting_lane %>
    <%= f.input :entry_count %>
    <%= f.input :paid %>
    <%= f.input :rejected_count %>
    <%= f.input :refunded %>
    <%= f.input :average %>
    <%= f.association :bowler %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
