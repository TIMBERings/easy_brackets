<%- model_class = Entry -%>
<div class="page-header">
  <h1><%=t '.title', :default => Entry %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:starting_lane) %></dt>
      <dd><pre class="prettyprint"><%= @entry.starting_lane %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:entry_count) %></dt>
      <dd><pre class="prettyprint"><%= @entry.entry_count %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:paid) %></dt>
      <dd><pre class="prettyprint"><%= @entry.paid %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:rejected_count) %></dt>
      <dd><pre class="prettyprint"><%= @entry.rejected_count %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:refunded) %></dt>
      <dd><pre class="prettyprint"><%= @entry.refunded %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:average) %></dt>
      <dd><pre class="prettyprint"><%= @entry.average %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              entries_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_entry_path(@entry), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              entry_path(@entry),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

